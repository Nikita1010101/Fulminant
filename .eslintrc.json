{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": "airbnb-base",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"linebreak-style": "off",
		"semi": "off",
		"indent": "off",
		"arrow-parens": "off",
		"implicit-arrow-linebreak": "off",
		"function-paren-newline": "off",
		"comma-dangle": "off",
		"arrow-body-style": "off",
		"camelcase": "off",
		"max-classes-per-file": [2, "warn"],
		"no-unused-vars": "warn",
		"no-param-reassign": "warn",
		"no-tabs": "off",
		"no-plusplus": "off",
		"no-useless-constructor": "off",
		"no-eval": "off",
		"no-continue": "off",
		"no-shadow": "off",
		"no-empty-function": "warn",
		"import/no-mutable-exports": "warn",
		"import/prefer-default-export": "off"
	}
}
